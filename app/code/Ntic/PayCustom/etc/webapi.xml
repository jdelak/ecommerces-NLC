<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/ntic-paycustom/config_payment_abo">
		<service class="Ntic\PayCustom\Api\ConfigPaymentAboRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_abo_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/ntic-paycustom/search">
		<service class="Ntic\PayCustom\Api\ConfigPaymentAboRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_abo_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/ntic-paycustom/:configPaymentAboId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentAboRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_abo_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/ntic-paycustom/:configPaymentAboId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentAboRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_abo_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/ntic-paycustom/:configPaymentAboId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentAboRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_abo_delete"/>
		</resources>
	</route>
	<route method="POST" url="/V1/ntic-paycustom/config_payment_fraction">
		<service class="Ntic\PayCustom\Api\ConfigPaymentFractionRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_fraction_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/ntic-paycustom/:configPaymentFractionId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentFractionRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_fraction_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/ntic-paycustom/:configPaymentFractionId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentFractionRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_fraction_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/ntic-paycustom/:configPaymentFractionId">
		<service class="Ntic\PayCustom\Api\ConfigPaymentFractionRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Ntic_PayCustom::config_payment_fraction_delete"/>
		</resources>
	</route>
</routes>
