<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">ntic_config_payment_fraction_form.config_payment_fraction_form_data_source</item>
			<item name="deps" xsi:type="string">ntic_config_payment_fraction_form.config_payment_fraction_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="config" xsi:type="array">
			<item name="dataScope" xsi:type="string">data</item>
			<item name="namespace" xsi:type="string">ntic_config_payment_fraction_form</item>
		</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
		<item name="buttons" xsi:type="array">
			<item name="back" xsi:type="string">Ntic\PayCustom\Block\Adminhtml\Config\Payment\Fraction\Edit\BackButton</item>
			<item name="delete" xsi:type="string">Ntic\PayCustom\Block\Adminhtml\Config\Payment\Fraction\Edit\DeleteButton</item>
			<item name="save" xsi:type="string">Ntic\PayCustom\Block\Adminhtml\Config\Payment\Fraction\Edit\SaveButton</item>
			<item name="save_and_continue" xsi:type="string">Ntic\PayCustom\Block\Adminhtml\Config\Payment\Fraction\Edit\SaveAndContinueButton</item>
		</item>
	</argument>
	<dataSource name="config_payment_fraction_form_data_source">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Ntic\PayCustom\Model\Configpaymentfraction\DataProvider</argument>
			<argument name="name" xsi:type="string">config_payment_fraction_form_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">config_payment_fraction_id</argument>
			<argument name="requestFieldName" xsi:type="string">config_payment_fraction_id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="submit_url" path="*/*/save" xsi:type="url"/>
				</item>
			</argument>
		</argument>
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
	</dataSource>
	<fieldset name="General">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="label" xsi:type="string"/>
			</item>
		</argument>
		<field name="amount">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">amount</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">50</item>
					<item name="dataScope" xsi:type="string">amount</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="operateur">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">operateur</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">60</item>
					<item name="dataScope" xsi:type="string">operateur</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="fraction">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">fraction</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">70</item>
					<item name="dataScope" xsi:type="string">fraction</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="to_type">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">to_type</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">80</item>
					<item name="dataScope" xsi:type="string">to_type</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="product_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">product_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">90</item>
					<item name="dataScope" xsi:type="string">product_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="store_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">store_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">config_payment_fraction</item>
					<item name="sortOrder" xsi:type="number">10</item>
					<item name="dataScope" xsi:type="string">store_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
	</fieldset>
</form>
