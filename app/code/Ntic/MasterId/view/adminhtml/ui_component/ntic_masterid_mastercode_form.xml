<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">ntic_masterid_mastercode_form.mastercode_form_data_source</item>
			<item name="deps" xsi:type="string">ntic_masterid_mastercode_form.mastercode_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="config" xsi:type="array">
			<item name="dataScope" xsi:type="string">data</item>
			<item name="namespace" xsi:type="string">ntic_masterid_mastercode_form</item>
		</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
		<item name="buttons" xsi:type="array">
			<item name="back" xsi:type="string">Ntic\MasterId\Block\Adminhtml\MasterCode\Edit\BackButton</item>
			<item name="delete" xsi:type="string">Ntic\MasterId\Block\Adminhtml\MasterCode\Edit\DeleteButton</item>
			<item name="save" xsi:type="string">Ntic\MasterId\Block\Adminhtml\MasterCode\Edit\SaveButton</item>
			<item name="save_and_continue" xsi:type="string">Ntic\MasterId\Block\Adminhtml\MasterCode\Edit\SaveAndContinueButton</item>
		</item>
	</argument>
	<dataSource name="mastercode_form_data_source">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Ntic\MasterId\Model\MasterCode\DataProvider</argument>
			<argument name="name" xsi:type="string">mastercode_form_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">mastercode_id</argument>
			<argument name="requestFieldName" xsi:type="string">mastercode_id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="submit_url" path="*/*/save" xsi:type="url"/>
				</item>
			</argument>
		</argument>
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
	</dataSource>
	<fieldset name="General">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="label" xsi:type="string"/>
			</item>
		</argument>
		<field name="nav_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">nav_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">MasterCode</item>
					<item name="sortOrder" xsi:type="number">10</item>
					<item name="dataScope" xsi:type="string">nav_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="customer_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">customer_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">MasterCode</item>
					<item name="sortOrder" xsi:type="number">20</item>
					<item name="dataScope" xsi:type="string">customer_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="well_master">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">well_master</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">MasterCode</item>
					<item name="sortOrder" xsi:type="number">30</item>
					<item name="dataScope" xsi:type="string">well_master</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="int_master">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">int_master</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">MasterCode</item>
					<item name="sortOrder" xsi:type="number">40</item>
					<item name="dataScope" xsi:type="string">int_master</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
	</fieldset>
</form>
