<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">ntic_subscription_contract_form.contract_form_data_source</item>
			<item name="deps" xsi:type="string">ntic_subscription_contract_form.contract_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="config" xsi:type="array">
			<item name="dataScope" xsi:type="string">data</item>
			<item name="namespace" xsi:type="string">ntic_subscription_contract_form</item>
		</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
		<item name="buttons" xsi:type="array">
			<item name="back" xsi:type="string">Ntic\Subscription\Block\Adminhtml\Contract\Edit\BackButton</item>
			<item name="delete" xsi:type="string">Ntic\Subscription\Block\Adminhtml\Contract\Edit\DeleteButton</item>
			<item name="save" xsi:type="string">Ntic\Subscription\Block\Adminhtml\Contract\Edit\SaveButton</item>
			<item name="save_and_continue" xsi:type="string">Ntic\Subscription\Block\Adminhtml\Contract\Edit\SaveAndContinueButton</item>
		</item>
	</argument>
	<dataSource name="contract_form_data_source">
		<argument name="dataProvider" xsi:type="configurableObject">
			<argument name="class" xsi:type="string">Ntic\Subscription\Model\Contract\DataProvider</argument>
			<argument name="name" xsi:type="string">contract_form_data_source</argument>
			<argument name="primaryFieldName" xsi:type="string">contract_id</argument>
			<argument name="requestFieldName" xsi:type="string">contract_id</argument>
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="submit_url" path="*/*/save" xsi:type="url"/>
				</item>
			</argument>
		</argument>
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
	</dataSource>
	<fieldset name="General">
		<argument name="data" xsi:type="array">
			<item name="config" xsi:type="array">
				<item name="label" xsi:type="string"/>
			</item>
		</argument>
		<field name="store_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">store_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">10</item>
					<item name="dataScope" xsi:type="string">store_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">false</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="order_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">order_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">20</item>
					<item name="dataScope" xsi:type="string">order_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="date_suscription">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">date_suscription</item>
					<item name="formElement" xsi:type="string">date</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">30</item>
					<item name="dataScope" xsi:type="string">date_suscription</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="month_subscription">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">month_subscription</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">40</item>
					<item name="dataScope" xsi:type="string">month_subscription</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="day_subscription">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">day_subscription</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">50</item>
					<item name="dataScope" xsi:type="string">day_subscription</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="honored">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">honored</item>
					<item name="formElement" xsi:type="string">checkbox</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">60</item>
					<item name="dataScope" xsi:type="string">honored</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="data_nav">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">data_nav</item>
					<item name="formElement" xsi:type="string">textarea</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">70</item>
					<item name="dataScope" xsi:type="string">data_nav</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="indice">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">indice</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">80</item>
					<item name="dataScope" xsi:type="string">indice</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="status">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">status</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">90</item>
					<item name="dataScope" xsi:type="string">status</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
		<field name="order_nav_id">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="dataType" xsi:type="string">text</item>
					<item name="label" translate="true" xsi:type="string">order_nav_id</item>
					<item name="formElement" xsi:type="string">input</item>
					<item name="source" xsi:type="string">Contract</item>
					<item name="sortOrder" xsi:type="number">100</item>
					<item name="dataScope" xsi:type="string">order_nav_id</item>
					<item name="validation" xsi:type="array">
						<item name="required-entry" xsi:type="boolean">true</item>
					</item>
				</item>
			</argument>
		</field>
	</fieldset>
</form>
