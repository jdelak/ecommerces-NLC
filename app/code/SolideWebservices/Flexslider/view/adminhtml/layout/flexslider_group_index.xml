<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<title>Flexslider - Group Management</title>
	</head>
	<update handle="styles" />
	<body>
		<referenceBlock name="menu">
			<action method="setActive">
				<argument name="itemId" xsi:type="string">SolideWebservices_Flexslider::group</argument>
			</action>
		</referenceBlock>
		<referenceBlock name="page.title">
			<action method="setPageTitle">
				<argument name="pageTitle" xsi:type="string">Flexslider - Group Management</argument>
			</action>
		</referenceBlock>
		<referenceContainer name="content">
			<uiComponent name="flexslider_group_listing" />
		</referenceContainer>
	</body>
</page>
